# Copy this file to .env and replace the values with your specific configuration

# GitHub Personal Access Token for accessing GitHub APIs
GITHUB_TOKEN=<your_github_personal_access_token>

# Owner of the GitHub repository (username or organization)
GITHUB_REPO_OWNER=<github_repository_owner>

# Name of the GitHub repository
GITHUB_REPO_NAME=<github_repository_name>

# Branch of the GitHub repository to read from (default is usually "main")
GITHUB_REPO_BRANCH=<github_repository_branch>

# OpenAI API key for accessing OpenAI services
OPENAI_API_KEY=<your_openai_api_key_here>
